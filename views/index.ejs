    <link rel="stylesheet" href="stylesMain.css">
  </head>
  <body>
    
    <div id="particles-js" class="snow"></div>

    <main>
      <!-- <img src="https://github.com/ecemgo/mini-samples-great-tricks/assets/13468728/64ef8d9a-9202-4543-b838-82f4c7c91ccf" alt="" /> -->
