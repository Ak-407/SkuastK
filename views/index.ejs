        <div class="carousel-inner">
          <div class="carousel-item active">
            <div><img class="d-block w-100"src="pexels-james-wheeler-1487010.jpg" alt="First slide"></div>
          </div>
          
          
          <div class="carousel-item">
            <div><img class="d-block w-100" src="pexels-siegfried-poepperl-7474927.jpg" alt="First slide"></div>
          </div>
          <div class="carousel-item">
            <div><img class="d-block w-100" src="pexels-david-kooijman-3591322.jpg" alt="First slide"></div>
          </div>
          <div class="carousel-item">
            <div><img class="d-block w-100" src="pxfuel.jpg" alt="First slide"></div>
          </div>
    
          <div class="carousel-item">
            <div><img class="d-block w-100" src="1693577379922.jpg" alt="Second slide"></div>
          </div>
          <div class="carousel-item">
            <div><img class="d-block w-100"src="1693577460410.jpg" alt="Third slide"></div>
          </div>
          <div class="carousel-item">
            <div><img class="d-block w-100"src="1693577553592.jpg" alt="Third slide"></div>
          </div>
          <div class="carousel-item">
            <div><img class="d-block w-100"src="1693577822858.jpg" alt="Third slide"></div>
          </div>
        </div>
        <a style="border-radius: 100px;" class="carousel-control-prev" href="#carouselExampleDark" role="button" data-slide="prev">
          <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        </a>
        <a style="border-radius: 100px;" class="carousel-control-next" href="#carouselExampleDark" role="button" data-slide="next">
          <span class="carousel-control-next-icon" aria-hidden="true"></span>
        </a>
      </div>
      <div>
          <h1 class="h1Head">Select Experiment</h1>
          <form action="/login" method="post">
              <input type="hidden" name="StudentExperiment" value="Student Experiment">

              <div class="buttons">

              <div class="buttons">

                  <button class="raise LLL" type="submit">Student Experiment</button>
                </div>
                </div>
          </form>
          <form action="/login" method="post">
              <input type="hidden" name="StudentExperiment" value="RCM Project">

              <div class="buttons">

              <div class="buttons">

                  <button class="raise LL" type="submit">RCM Project</button>
                </div>
                </div>
          </form>
          <form action="/login" method="post">
              <input  id="OO" type="hidden" name="StudentExperiment" value="Externally Funded Project">

              <div class="buttons">

              <div class="buttons">

                  <button class="raise  L" type="submit">Externally Funded Project</button>
                </div>
                </div>
          </form>
      </div>
    </main>
  </body>
  <script
      src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
